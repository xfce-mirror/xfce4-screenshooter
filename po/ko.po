msgid "<span weight=\"bold\" stretch=\"semiexpanded\">Options</span>"
msgstr "<span weight=\"bold\" stretch=\"semiexpanded\">옵션</span>"

#: lib/screenshooter-dialogs.c:1020
msgid "Capture the mouse pointer"
msgstr "마우스 포인터 찍기"

